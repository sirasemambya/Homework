function ClearCallFields()
{
	with (oCallScriptForm)
	{
		SCRIPT.value		= "";
		ACTION.value		= "";
		AUDITTYPE.value		= "";
		DEGREETERM.value	= "";
		INTNOTES.value		= "";
		REPORT.value		= "";
		INPROGRESS.value	= "";
		CUTOFFTERM.value	= "";
		NOTENUM.value		= "";
		NOTETEXT.value		= "";
		NOTEMODE.value		= "";
		INTERNAL.value		= ""; //1.0b
		PENDING.value		= ""; //1.0b
		CLASSES.value		= ""; //1.0j
		ContentType.value   = "";
	}
}

