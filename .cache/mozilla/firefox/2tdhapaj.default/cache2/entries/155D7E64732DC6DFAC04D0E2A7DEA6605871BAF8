<script language="javascript" src='/uicomponents/scripts/njitscript.js'></script>            <script LANGUAGE="JavaScript">function linkPopUp(url) {
			var newURL = url;
			var currentURL = top.document.location.href;
			var firstChar = url.charAt(0);

			if(firstChar =="/"){
				sindex = currentURL.indexOf("/",7)
				if(sindex >0){
					url1 = currentURL.substring(0, sindex);
					newURL = url1 + url;
				}
			}
				linkPopUpWindow=window.open(newURL,"NJIT", "resizable=yes,toolbar=yes,scrollbars=yes,menubar=yes,status=yes");
				linkPopUpWindow.focus();
		}
		</script>
